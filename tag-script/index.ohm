/*
  Testing only tags
*/
TagScript {
  tags = row*
  row = indent? closingTags pairWithTag eol?
  pairWithTag = indent freeTextNoSpaces
  freeTextNoSpaces = ( ~space any )*
  indent = space*
  closingTags = 
    "html"
    | "div"
    | "span"
    | "p"
    | "label"

  inlineTags = 
    "img" 

  plainText = "|" freeTextNoSpaces

  eol = "\n"
}
