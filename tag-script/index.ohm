/*
  Testing only tags
*/
TagScript {
  tags = row (eol ~end row)* eol?
  row = indent? closingTags
  closingTags = manyFreeTextNoSpaces
  pairWithTag = indent textInline?
  textInline = any (~eol any )* eol?

  manyFreeTextNoSpaces = ( ~space any )*
  freeTextNoSpaces = ~space any

  indent = space*

  inlineTags = 
    "img" 
  plainText = "|" freeTextNoSpaces

  eol = "\n" 
}
